<section class="panel" id="header">
  <h2>Header</h2>
  <div class="form-group">
    <label>Name <input id="headerName"/></label>
  </div>
  <div class="form-group">
    <label>Email <input id="headerEmail" type="email"/></label>
  </div>
  <div class="form-group">
    <label>Address <input id="headerAddress"/></label>
  </div>
  <div class="form-group">
    <label>Whatsapp <input id="headerWhatsapp" placeholder="(11) 98765-4321" inputmode="numeric"/></label>
  </div>
  <div class="form-group item">
    <label>Github URL <input id="headerGithubUrl" placeholder="https://github.com/..."/></label>
    <label>Github Display <input id="headerGithubDisplay" placeholder="github.com/username"/></label>
  </div>
  <div class="form-group">
    <label>LinkedIn URL <input id="headerLinkedinUrl" placeholder="https://linkedin.com/in/..."/></label>
    <label>LinkedIn Display <input id="headerLinkedinDisplay" placeholder="linkedin.com/in/username"/></label>
  </div>
  <div class="form-group">
    <label>Personal Links</label>
    <div id="personalLinks" class="items-container"></div>
    <button type="button" id="personalLinksAdd" class="btn-add">+ Add Personal Link</button>
  </div>
</section>
